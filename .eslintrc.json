{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },

  "plugins": ["import"],

  "extends": ["plugin:prettier/recommended"],

  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
